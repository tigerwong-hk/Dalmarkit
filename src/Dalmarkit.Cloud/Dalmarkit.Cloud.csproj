<Project Sdk="Microsoft.NET.Sdk">

  <ItemGroup>
    <ProjectReference Include="..\Dalmarkit.Common\Dalmarkit.Common.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="14.0.0" />
    <PackageReference Include="AWSSDK.CloudFront" Version="3.7.406" />
    <PackageReference Include="AWSSDK.CloudWatchEvents" Version="3.7.400.179" />
    <PackageReference Include="AWSSDK.CognitoIdentityProvider" Version="3.7.409.28" />
    <PackageReference Include="AWSSDK.Lambda" Version="3.7.413.13" />
    <PackageReference Include="AWSSDK.Route53" Version="3.7.409.6" />
    <PackageReference Include="AWSSDK.S3" Version="3.7.421.4" />
    <PackageReference Include="AWSSDK.SimpleSystemsManagement" Version="3.7.406.8" />
    <PackageReference Include="Roslynator.Analyzers" Version="4.13.1">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <PackageId>Dalmarkit.Cloud</PackageId>
    <PackageTags>Dalmarkit AWS Cloud CloudFront Cognito Route53 S3</PackageTags>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AnalysisLevel>latest-recommended</AnalysisLevel>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

</Project>
